{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "lib": ["ES2023", "ES2023.Array", "ES2023.Collection"],
    "traceResolution": true,
    "noImplicitOverride": true
  },
  "include": ["src/**/*.ts", "*.js"]
}
